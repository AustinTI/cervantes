@using Cervantes.Web.Localization
@using Microsoft.Extensions.Localization
@inject IStringLocalizer<Resource> localizer


<MudGrid>
<MudItem xs="12" sm="12" md="12" lg="12">
    @switch (@Risk)
    {
        case "LOW":
            <MudPaper Height="250" Class="pa-4 mud-success">
                <MudText Typo="Typo.h6" Align="Align.Center">@localizer["low"]</MudText>
                <MudDivider/>
                <MudText Align="Align.Center">@Vector</MudText>
            </MudPaper>
            break;
        case "MEDIUM":
            <MudPaper Height="250" Class="pa-4 mud-warning">
                <MudText Typo="Typo.h6" Align="Align.Center">@localizer["medium"]</MudText>
                <MudDivider/>
                <MudText Align="Align.Center">@Vector</MudText>
            </MudPaper>
            break;
        case "HIGH":
            <MudPaper Height="250" Style="@($"color:{Colors.DeepOrange.Lighten5}; background:{Colors.DeepOrange.Default};")" Class="pa-4">
                <MudText Typo="Typo.h6" Align="Align.Center">@localizer["high"]</MudText>
                <MudDivider/>
                <MudText Align="Align.Center">@Vector</MudText>
            </MudPaper>
            break;
        case "CRITICAL":
            <MudPaper Height="250" Class="pa-4 mud-secondary">
                <MudText Typo="Typo.h6" Align="Align.Center">@localizer["critical"]</MudText>
                <MudDivider/>
                <MudText Align="Align.Center">@Vector</MudText>
            </MudPaper>
            break;
        default:
            <MudPaper Height="250" Class="pa-4 mud-info">
                <MudText Typo="Typo.h6" Align="Align.Center">N/A</MudText>
                <MudDivider/>
                <MudText Align="Align.Center">@Vector</MudText>
            </MudPaper>
            break;
    }

</MudItem>


